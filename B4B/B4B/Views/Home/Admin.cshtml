@*Add the model you want to use*@
@model IEnumerable<B4B.Models.Profile>

    @{
    Layout = "~/Views/Shared/_Layout.cshtml";
    }


    @*Go into the controller*@

    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="createbutton">
                    @Html.ActionLink("Click to create a new profile", "Create", "Profiles", null, new { @class = "btn btn-default"})
                </div>


                @*<button id="addProfile" class="btn btn-default">+</button>*@


                @foreach (var item in Model)
            {
                    <div class="adminHome">
                        <img src="~/File?id=@item.ProfileID" class="idpicsize img-responsive" />
                    </div>
            }


            </div>
        </div>

    </div>
